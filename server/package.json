{
  "name": "library-app-backend",
  "version": "1.0.0",
  "description": "Backend for app ",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "dev": "nodemon index.js",
    "db:init": "docker exec -i server-db psql -U ayman -d library_db",
    "db:reset": "docker exec server-db psql -U ayman -d library_db -c 'DROP SCHEMA IF EXISTS public CASCADE; CREATE SCHEMA public;' && npm run db:init",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
